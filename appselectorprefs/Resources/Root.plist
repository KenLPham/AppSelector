<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<!-- Title -->
		<dict>
			<key>cell</key> 					<string>PSGroupCell</string>
			<key>headerCellClass</key> 			<string>HBPackageNameHeaderCell</string>
			<key>packageIdentifier</key> 		<string>com.kayfam.appselector</string>
			<key>showAuthor</key> 				<false/>
		</dict>

		<!-- Plug -->
		<dict>
			<key>cellClass</key> 			<string>HBTwitterCell</string>
			<key>label</key> 				<string>Ken</string>
			<key>user</key> 				<string>lilboipham</string>
		</dict>

		<dict>
			<key>cell</key> 				<string>PSGroupCell</string>
		</dict>
		<!-- Respring -->
        <dict>
            <key>cell</key>         	    <string>PSButtonCell</string>
            <key>label</key>       		    <string>Respring</string>
            <key>action</key>      	   		<string>respring</string>
        </dict>

        <!-- Settings -->
		<dict>
			<key>cell</key> 				<string>PSGroupCell</string>
			<key>label</key> 				<string>Settings</string>
		</dict>
		<!-- Enable the Tweak -->
		<dict>
			<key>cell</key> 				<string>PSSwitchCell</string>
			<key>label</key> 				<string>Enable</string>
			<key>key</key> 					<string>enabled</string>
			<key>defaults</key> 			<string>com.kayfam.appselector</string>
			<key>default</key> 				<true/>
		</dict>
		<!-- Use App Image -->
		<dict>
			<key>cell</key> 				<string>PSSwitchCell</string>
			<key>label</key> 				<string>Use App Image</string>
			<key>key</key> 					<string>image</string>
			<key>defaults</key> 			<string>com.kayfam.appselector</string>
			<key>default</key> 				<true/>
		</dict>
		<!-- Switch Quick Select App when a different app is opened -->
		<dict>
			<key>cell</key> 				<string>PSSwitchCell</string>
			<key>label</key> 				<string>Quick Switch</string>
			<key>key</key> 					<string>quick</string>
			<key>defaults</key> 			<string>com.kayfam.appselector</string>
			<key>default</key> 				<true/>
		</dict>
		<!-- Method -->
		<dict>
			<key>cell</key> 				<string>PSLinkListCell</string>
			<key>label</key> 				<string>Method</string>
			<key>key</key> 					<string>function</string>
			<key>defaults</key> 			<string>com.kayfam.appselector</string>
			<key>default</key> 				<string>default</string>
			<key>detail</key> 				<string>PSListItemsController</string>
			<key>validValues</key>
			<array>
				<string>default</string>
				<string>camera</string>
				<string>force</string>
			</array>

			<key>validTitles</key>
			<array>
				<string>App Icon opens Photos</string>
				<string>Camera opens Photos</string>
				<string>Force touch Camera opens Photos</string>
			</array>

			<key>shortTitles</key>
			<array>
				<string>Default</string>
				<string>Camera</string>
				<string>Force Touch</string>
			</array>
		</dict>

		<!-- Support -->
		<dict>
			<key>cell</key> 				<string>PSGroupCell</string>
			<key>label</key> 				<string>Support</string>
		</dict>
		<!-- PayPal -->
		<!-- https://www.paypal.com/cgi-bin/webscr?cmd=_donations&amp;business=TBB3GGAEF9GJA&amp;currency_code=USD&amp;source=url -->
		<dict>
			<key>cellClass</key> 			<string>HBLinkTableCell</string>
			<key>label</key> 				<string>PayPal</string>
			<key>subtitle</key> 			<string>Help feed me</string>
			<key>url</key> 					<string>https://bit.ly/2FVyWNm</string>
		</dict>
		<!-- Source -->
		<dict>
			<key>cellClass</key> 			<string>HBLinkTableCell</string>
			<key>label</key> 				<string>GitHub</string>
			<key>subtitle</key> 			<string>Source Code</string>
			<key>url</key> 					<string>https://github.com/KenLPham/AppSelector/</string>
		</dict>
	</array>
	<key>title</key>
	<string>App Selector</string>
</dict>
</plist>
